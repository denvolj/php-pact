{
    "name": "pact-im/sdk-php",
    "type": "library",
    "description": "PHP library for pact.im",
    "keywords": ["library", "pact"],
    "homepage": "https://pact.im",
    "license": "MIT",
    "authors": [
        {
            "name": "Pact LLC",
            "homepage": "https://github.com/pact-im/sdk-php"
        }
    ],
    "require": {
      "php": ">=7.3.0",
      "ext-curl": "*",
      "ext-json": "*",
        "nyholm/psr7": "^1.3",
        "kriswallsmith/buzz": "^1.1",
        "php-http/multipart-stream-builder": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Pact\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "phpdocumentor/phpdocumentor": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Pact\\Tests\\": "tests/"
        }
    }
}
